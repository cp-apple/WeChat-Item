/* 我的办理 */
page{
  background-color: #f2f2f2;
}
.item{
  margin: 10px 15rpx;
  background-color: #fff;
  border: 1px solid #ddd;
  border-radius: 5px;
}
.itemTIt{
  padding: 10px 0;
  border-bottom: 1px solid #ddd;
}
.itemTIt image{
  margin: 0 15rpx;
  width: 35rpx;
  height: 35rpx;
}
.itemTIt .rImg{
  float: right;
  margin-top: 5px;
  margin-left: 20rpx;
  width: 35rpx;
  height: 20rpx;
}
.itemState{
  float: right;
}

.itemCon{
  padding: 10px 80rpx;
}
.myBussLog{
  margin-top: 5px;
}
.myBussAddr{
  position: relative;
  display: block;
  font-size: 16px;
}
.myBussAddr:before{
  content: '';
  display: block;
  position: absolute;
  top: 12px;
  left: -40rpx;
  width: 30rpx;
  height: 2px;
  background-color: #F24E22;
}
.itemCon text{
  line-height: 2
}

.classname{
  position: relative;
  padding-left: 30rpx;
  border-left: 2px solid #ddd;
}
.classname:before{
  content: '';
  display: block;
  position: absolute;
  top: 0;
  left: -6px;
  width: 10px;
  height: 10px;
  background-color: #ddd;
  border-radius: 50%;
}
.classname:nth-child(1){
  color: #2DA960;
}
.classname:nth-child(1):before{
   background-color: #2DA960;
}
.classname:last-child{
  border-color: #fff;
}
.classname text{
  position: relative;
  top: -8px;
  font-size: 12px;
}