/* 常见问题 */
page{
 background-color: #f2f2f2;
}
.problems{
  margin: 20px 0;
  padding: 0 20rpx;
  border-top: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
  background-color: #fff;
}
.aFAQ{
  position: relative;
  padding: 10px 0;
  padding-right: 40rpx;
  border-bottom: 1px solid #ddd;
  background-color: #fff;
}
.rightImg{
  position: absolute;
  top: 36rpx;
  right: 0;
  width: 15rpx;
  height: 20rpx;
}
.proImg{
  position: absolute;
  top: 20rpx;
  left: 0;
  width: 38rpx;
  height: 40rpx;
}
.aFAQ:last-child{
  border-bottom: 0;
}

.aFAQ text{
  display: block;
  padding-left: 50rpx;
  font-size: 16px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  background-color: #fff;
}